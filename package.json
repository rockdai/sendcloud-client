{
  "name": "sendcloud-client",
  "version": "0.1.1",
  "description": "Node.js client for http://sendcloud.sohu.com",
  "main": "index.js",
  "scripts": {
    "test": "mocha test/*.test.js --timeout 15000 --require should",
    "test-cov": "node --harmony node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --check-leaks -t 15000 test/*.test.js",
    "test-travis": "node --harmony node_modules/.bin/istanbul cover node_modules/.bin/_mocha --report lcovonly -- --check-leaks -t 15000 test/*.test.js",
    "cnpm": "npm install --registry=https://registry.npm.taobao.org"
  },
  "dependencies": {
    "urllib-sync": "~1.1.2"
  },
  "devDependencies": {
    "istanbul": "~0.4.0",
    "mocha": "~2.3.4",
    "should": "~7.1.1"
  },
  "homepage": "https://github.com/rockdai/sendcloud-client",
  "repository": {
    "type": "git",
    "url": "git://github.com/rockdai/sendcloud-client.git",
    "web": "https://github.com/rockdai/sendcloud-client"
  },
  "bugs": {
    "url": "https://github.com/rockdai/sendcloud-client/issues",
    "email": "rockdai@qq.com"
  },
  "keywords": [
    "sendcloud", "mail"
  ],
  "engines": {
    "node": ">= 4.2.1"
  },
